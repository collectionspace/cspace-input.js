{
  "name": "cspace-input",
  "version": "1.3.0",
  "description": "CollectionSpace input components",
  "author": "Ray Lee <ray.lee@lyrasis.org>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cspace",
    "ui",
    "input",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/collectionspace/cspace-input.js.git"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "styles",
    "images"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-umd",
    "build-cjs": "cross-env BABEL_ENV=cjs babel ./src -d lib",
    "build-umd": "npm run build-umd-dev && npm run build-umd-prod",
    "build-umd-dev": "webpack",
    "build-umd-prod": "cross-env NODE_ENV=production webpack",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf lib dist coverage",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "lint": "eslint --ext .jsx,.js *.js src test examples",
    "prepare": "npm run clean && npm run build",
    "start": "cross-env NODE_ENV=test karma start",
    "test": "cross-env NODE_ENV=test karma start --singleRun=true"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cspace-layout": "^1.0.1",
    "cspace-refname": "^1.0.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-calendar": "^2.18.1",
    "react-dom": "^15.6.2",
    "react-quill": "^1.3.1",
    "sugar-date": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^3.0.2",
    "cross-env": "^2.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "file-loader": "^0.9.0",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.6",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "postcss-loader": "^2.1.6",
    "postcss-modules-values": "^1.3.0",
    "react-shallow-testutils": "^3.0.1",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
